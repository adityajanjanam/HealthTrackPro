{
  "name": "healthtrackpro",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "newArchEnabled": true,
  "assetBundlePatterns": [
    "**/*",
    "assets/fonts/*"
  ],
  "scripts": {
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start-frontend": "npx expo start --android",
    "start-backend": "cd backend && npm run dev",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-native-community/cli": "^15.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "^2.10.0",
    "@react-navigation/native": "^7.0.7",
    "@react-navigation/stack": "^7.1.1",
    "@react-native-camera-roll/camera-roll": "^8.0.0",
    "@react-native-document-picker/document-picker": "^9.2.2",
    "@sentry/react-native": "^8.50.0",
    "axios": "^1.7.9",
    "expo": "~53.0.0",
    "expo-app-loading": "^2.1.1",
    "expo-auth-session": "^7.0.11",
    "expo-crypto": "~14.0.1",
    "expo-device": "~7.0.1",
    "expo-image-picker": "~16.0.5",
    "expo-linear-gradient": "~14.0.2",
    "expo-notifications": "~0.29.12",
    "expo-splash-screen": "^0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-web-browser": "^15.0.11",
    "expo-file-system": "~17.0.1",
    "expo-print": "~13.0.0",
    "react": "^18.3.1",
    "react-native": "^0.77.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.21.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.16.3",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "~4.3.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-svg": "^15.8.0",
    "react-dom": "^18.3.1",
    "react-native-web": "~0.19.13",
    "@expo/metro-runtime": "~3.3.0",
    "date-fns": "^4.1.0",
    "@react-native-community/netinfo": "^11.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@types/react-native": "^0.73.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
