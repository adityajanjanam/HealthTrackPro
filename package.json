{
  "name": "healthtrackpro",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "newArchEnabled": true,
  "assetBundlePatterns": [
    "**/*",
    "assets/fonts/*"
  ],
  "scripts": {
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
